[elastix-base]
name=Base RPM Repository for Elastix 
mirrorlist=http://mirror.elastix.org/?release=1&arch=$basearch&repo=base
#baseurl=http://repo.elastix.org/elastix/1/base/$basearch/
gpgcheck=1
enabled=1
exclude=elastix-mysqldbdata
gpgkey=http://repo.elastix.org/elastix/RPM-GPG-KEY-Elastix

